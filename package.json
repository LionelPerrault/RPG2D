{
  "name": "navarra",
  "version": "0.0.1",
  "description": "Top-down 2D Medieval RPG",
  "author": "Dan Jasnowski <djasnowski@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "node server.js",
    "postinstall": "npm run build",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "build": "node build/build.js",
    "format": "eslint --fix",
    "server_dev": "./node_modules/.bin/nodemon server",
    "server_debug": "./node_modules/.bin/ndb server",
    "server_prod": "./node_modules/.bin/pm2 start server",
    "server_prod_restart": "./node_modules/.bin/pm2 restart all"
  },
  "dependencies": {
    "express": "^4.16.2",
    "ndb": "^1.0.24",
    "pm2": "^3.0.4",
    "vue": "^2.5.2",
    "ws": "^6.0.0",
    "node-emoji": "^1.8.1",
    "node-uuid": "^1.4.8"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "axios": "^0.17.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.3.0",
    "compression-webpack-plugin": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.8",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.0",
    "jest-serializer-vue": "^0.3.0",
    "lodash": "^4.17.4",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "pathfinding": "^0.4.18",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "stylelint": "^8.3.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.5.8",
    "vue-jest": "^1.0.2",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.10.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "nodemonConfig": {
    "env": {
      "SITE_URL": "http://website.test"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
